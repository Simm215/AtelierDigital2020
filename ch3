public class Main {
    public static void main(String[] args) {
        int []array= new int[]{-3, 0, 2, 3, -2};
        int pairs=0;
        int i;
        for(i=0; i<array.length-1;i++)
        {for(int j=0; j<array.length;j++)
            if(array[i]+array[j]==0) {
                pairs++;
                array[j] = array[j + 1];
                continue;
            }

          }
        System.out.println(pairs);
    }

}
